n = int(input("Enter the number of elements: "))
nums = [int(input(f"Enter element {i+1}: ")) for i in range(n)]

k = 0
for num in nums:
    if k == 0 or num != nums[k-1]:
        nums[k] = num
        k += 1

print("Number of unique elements:", k)
print("Array with unique elements:", nums[:k] + ["_"] * (len(nums)-k))
