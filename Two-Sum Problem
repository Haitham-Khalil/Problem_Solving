target = int(input("Enter target value: "))
numsLength=int(input("Enter the desired length for the List of the integers: "))

print("Now fill in the list (the number of your inputs is equal to the desired length you just entered)")
nums = [int(input()) for _ in range(numsLength)] # Creating a list of integers 
arrPos=[] # Array of positions


for i in range(len(nums)):
    
    for j in range(i+1,len(nums)):
        if(nums[i]+nums[j]==target):
            arrPos.append(i)
            arrPos.append(j)
            break # breaking out of the inner for loop
    break # breaking out of the outer for loop
# Goal is to find the first pair of positions
            
for pos in arrPos:
    print(pos)
            
        
    
